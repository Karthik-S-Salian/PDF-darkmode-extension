{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "1.0",
    "icons": {
        "16": "icons/icons8-pdf-pulsar-color-16.png",
        "32": "icons/icons8-pdf-pulsar-color-32.png",
        "96": "icons/icons8-pdf-pulsar-color-96.png"
    },

    "action": {
        "default_popup": "popup/popup.html"
    },

    "permissions": [
        "storage",
        "activeTab"
      ],

    "content_scripts":[
        {
            "matches": [
                "file://*/*.pdf"
            ],
            "js":[
                "content-scripts/content.js"
            ]
        }
    ]
}