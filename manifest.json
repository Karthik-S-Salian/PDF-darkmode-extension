{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "1.0",
    "icons": {
        "16": "icons/pdf.png",
        "24": "icons/pdf (1).png",
        "32": "icons/pdf (2).png",
        "64": "icons/pdf (3).png",
        "128": "icons/pdf (4).png"
    },

    "action": {
        "default_popup": "popup/popup.html"
    },

    "permissions": [
        "storage",
        "activeTab"
      ],

    "content_scripts":[
        {
            "matches": [
                "file://*/*.pdf"
            ],
            "js":[
                "content-scripts/content.js"
            ]
        }
    ]
}